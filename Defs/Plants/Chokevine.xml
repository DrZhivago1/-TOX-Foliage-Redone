<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
<ThingDef ParentName="PlantBase">
    <defName>Tox_Chokevine</defName>
    <label>chokevine</label>
    <description>Tangled masses of cord-like, spiky vines which coil in great masses along the ground. Chokevine greatky slows down anyone who moves over it. Multiple tribes have independently generated stories of chokevine strangling children to death in the night, as a way to scare kids into coming home before dark.</description>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Nutrition>0.25</Nutrition>
    </statBases>
    <pathCost>42</pathCost>
    <graphicData>
      <texPath>Plants/Chokevine</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <ingestible />
    <plant>
      <fertilitySensitivity>0.7</fertilitySensitivity>
      <growDays>5</growDays>
      <harvestWork>150</harvestWork>
      <maxMeshCount>16</maxMeshCount>
      <leaflessGraphicPath>Things/Plant/Chokevine_Leafless</leaflessGraphicPath>
      <visualSizeRange>0.3~0.6</visualSizeRange>
      <topWindExposure>0.16</topWindExposure>
      <wildClusterRadius>6</wildClusterRadius>
      <wildClusterWeight>200</wildClusterWeight>
      <wildOrder>1</wildOrder>
      <wildEqualLocalDistribution>false</wildEqualLocalDistribution>
	  <wildBiomes>
	   <ColdBog>1.5</ColdBog>
	   <TemperateSwamp>0.8</TemperateSwamp>
	   <TropicalRainforest>0.5</TropicalRainforest>
	   <TropicalSwamp>0.8</TropicalSwamp>
	</wildBiomes>
	<sowTags>
		<li>Ground</li>
	</sowTags>
    </plant>
  </ThingDef>
  
</Defs>
